#include <iostream>
using namespace std;

int main()
{
    //*****break*****
    for (int i = 0; i < 5; i++)
    {

        if (i == 3)
        {
            break;
        }
        cout << i << endl;
    }

    //*****contiue*****
    for (int i = 0; i < 5; i++)
    {

        if (i == 1)
        {
            continue;
        }
        cout << i << endl;
    }
    return 0;
}
