#include <iostream>
using namespace std;

int main()
{
    int a;
    cout << "Enter the number : " << a << endl;
    cin >> a;
    for (int i = 1; i <= 10; i++)
    {

        cout << "Multiplication table of " << a << endl;
        cout << a << " * " << i << " = " << i * a << endl;
    }
    return 0;
}
