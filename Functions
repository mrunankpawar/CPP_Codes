#include <iostream>
using namespace std;

//Function Prototype
//Syntax : type func_name (arguments);
int sum(int a, int b); //---> ACCEPTABLE
//int sum(int a, b); //--->  NOT ACCEPTABLE
//int sum(int , int); //---> ACCEPTABLE

int main()
{
    int num1, num2;
    // num1 and num2 are actual parameters
    cout << "Enter first number " << endl;
    cin >> num1;
    cout << "Enter second number " << endl;
    cin >> num2;
    cout << "The sum is : " << sum(num1, num2);
    return 0;
}

int sum(int a, int b)
{
    // a and b are formal parameters which will be taking values from the actual parameters num1 and num2
    int c = a + b;
    return c;
}
