#include<iostream>
using namespace std;

int main()
{
    int num1, num2;
    //"<<" insertion operator
    cout<<"Enter the value of num1: \n";
    //">>" extraction operator
    cin>>num1;
    cout<<"Enter the value of num2: \n";
    cin>>num2;
    cout<<"Num1 is "<<num1 ;
    cout<<"\nNum2 is "<<num2;
    cout<<"The sum is: "<<num1+num2;
    
    // sum = num1 + num2;
    // cout<<"\nSum is "<<sum;

    return 0;
}
